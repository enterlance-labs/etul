# my.telegram.org apps credentials
API_ID=
API_HASH=

# Bot token from @BotFather
BOT_TOKEN=

# Chat ID for logging (super group with topics and admin bot)
LOG_CHAT_ID=-100...

# Comma-separated list of source chats IDs to whitelist (empty = all)
WHITELIST_SOURCE_CHAT_IDS=

# Update debounce time in seconds
DEBOUNCE_SECONDS=10

# Topic reconcile interval in minutes
TOPIC_RECONCILE_MINUTES=5

# Database connection URL (default)
DATABASE_URL="postgresql://postgres:postgres@etul-db:5425/etul?schema=public"

# String session for the userbot account (see README for instructions)
STRING_SESSION=
